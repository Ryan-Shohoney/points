{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testMatch": ["<rootDir>/**/*.e2e-spec.ts"],
  "coverageDirectory": "<rootDir>/coverage",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": "<rootDir>/tsconfig.e2e.json",
      "astTransformers": {
        "before": ["<rootDir>/jest.e2e.transformer.js"]
      }
    }
  }
}
